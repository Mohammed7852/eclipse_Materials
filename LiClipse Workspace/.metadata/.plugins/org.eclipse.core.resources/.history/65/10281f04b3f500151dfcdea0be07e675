start  =[6,5,4,1,2,0,3,7,8]
goal   =[1,2,3,4,5,6,7,8,0]



def create_heuristic (current,goal):
    i=0 
    counter=0
    while i<len(current):
        if current[i]!=goal[i]:
            counter+=1
        i+=1   
    print("counter=",counter)  
 


      
        
            
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
     
     
     
    
     
     
     
    
    












