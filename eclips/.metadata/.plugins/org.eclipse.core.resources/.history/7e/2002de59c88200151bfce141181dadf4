package huffmand;

import java.util.*;
import  java.util.TreeMap;


public class hufmand {

	private  Scanner cin=new Scanner(System.in);
 	private  int size  , max   ; 
  	private Stack<List<Double>> s = new  Stack();
	
	void setlist(List<Double> l  )
	{ 
		s.push(new ArrayList(l));
		//System.out.println(l);
		l.sort(null);
		Collections.reverse(l);
		
		recursive(l);
		
		 
		System.out.println(s.pop());
		//System.out.println(l + "\n"+ s);
		//while(!s.isEmpty())
		//System.out.println(s);
		
	}//end of setlist
	
	void recursive(List<Double> l)
	{
		s.push(new ArrayList(l));
		
		if(l.size()==1)
		{
			
			return ;
		}
	 	
 		 List<Double> list = new ArrayList();
 		 list = l ;
 		 int sz = list.size();
 		 list.set(sz-2, l.get(sz-2)+l.get(sz-1));
 		 list.remove(sz-1);
 		 list.sort(null);
 		 Collections.reverse(list);
 		 
 		 recursive(list);
 		System.out.println(s.pop());
 	
 		// System.out.println(new ArrayList(list));
 		 
	}//end recursive
	
}
